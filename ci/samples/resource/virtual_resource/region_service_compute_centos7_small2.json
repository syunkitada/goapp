{
  "Kind": "RegionService",
  "Spec": {
    "Name": "small-centos-service",
    "Kind": "Compute",
    "Region": "kanto",
    "ExpireDate": -1,
    "Spec": {
      "SchedulePolicy": {
        "Replicas": 2
      },
      "NetworkPolicy": {
        "Version": 4,
        "Interfaces": 1,
        "AssignPolicy": "Affinity",
        "StaticNetworks": []
      },
      "Kind": "Qemu",
      "Restart": "Always",
      "Vcpus": 2,
      "Memory": 2048,
      "Disk": 10,
      "Image": "centos-7"
    }
  }
}
